function palindrome(str) {
  console.log(str);
  let strClone = str.slice();
  //console.log(strClone.match(/([\w])/g));
  let individualCharacters = strClone.match(/([\w])/g);
  console.log(individualCharacters);
  return true;
}

let result = palindrome("five|\_/|four");
console.log(result);
