\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\First Analysis
function palindrome(str) {
  console.log(str);
  let strClone = str.slice();
  console.log(strClone.match(/([\w])/g))
  return true;
}

let result = palindrome("0_0 (: /-\ :) 0-0");
console.log(result);
