  console.log(str.match(/(^[\w])([\w])([^aeiou])/));
  console.log(str.match(/(^[\w])([\w]+)([aeiou])/));
  console.log(str.replace(/(^[^aeiou])/, ""));
  console.log(str.concat("way"));
  //"consonant"
  return str;
}

